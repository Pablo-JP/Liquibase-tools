<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd ">
    <changeSet id="HU-00001_TAG_CRTB_TA_LOAN_TOKEN_VOID" author="1048168">
        <tagDatabase tag="HU-00001"/>
    </changeSet>
    <changeSet id="HU-00001_ID_CRTB_TA_LOAN_TOKEN_VOID" author="1048168">
        <createTable tablespace="USERS" remarks="Table to store Tokens of each pending Void requests" schemaName="SC_CREDIT" tableName="TA_LOAN_TOKEN_VOID">
            <column name="FI_LOAN_TOKEN_VOID_ID" type="NUMBER(15)" remarks="ID of the row corresponding TO the TOKEN to be used when voiding after requesting day">
                <constraints nullable="false"/>
            </column>
            <column name="FI_LOAN_OPERATION_VOID_ID" type="NUMBER(15)" remarks="ID of the Operation requesting Voiding of other Operation">
                <constraints nullable="false"/>
            </column>
            <column name="FI_LOAN_VOID_TOKEN" type="NUMBER(15)" remarks="TOKEN to be used when voiding after requesting day">
                <constraints nullable="false"/>
            </column>
            <column name="FC_USER" type="VARCHAR2(30)" defaultValueComputed="USER" remarks="">
                <constraints nullable="false"/>
            </column>
            <column name="FC_IP_ADDRESS" type="VARCHAR2(39)" defaultValueComputed="'127.0.0.1'" remarks="">
                <constraints nullable="false"/>
            </column>
            <column name="FD_CREATED_DATE" type="DATE" defaultValueComputed="SYSDATE" remarks="">
                <constraints nullable="false"/>
            </column>
            <column name="FD_MODIFICATION_DATE" type="DATE" defaultValueComputed="SYSDATE" remarks="">
                <constraints nullable="false"/>
            </column>
            <column name="FC_UUID_TRACKING" type="VARCHAR2(36)" remarks="">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
